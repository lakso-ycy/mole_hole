{
  "name": "fp-pso-fleksibel",
  "version": "1.0.0",
  "description": "A simple Mole and Hole game built with HTML, CSS, and JS.",
  "main": "instrumented/script.js",
  "scripts": {
    "pretest": "nyc instrument ./ ./instrumented",
    "serve": "http-server . -p 8080",
    "test": "cypress run --browser chrome",
    "coverage": "nyc report --reporter=html && nyc report --reporter=text && nyc report --reporter=lcov",
    "lint": "eslint . --ext .js --ignore-pattern coverage && stylelint \"**/*.css\" \"!coverage/**/*.css\" --ignore-path .gitignore",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "full": "npm run pretest && start cmd /k \"npm run serve\" && npm run test && npm run coverage"
  },
  "nyc": {
    "report-dir": "./coverage",
    "temp-dir": "./.nyc_output",
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true,
    "include": [
      " script.js"
    ],
    "exclude": [
      "cypress/**",
      "node_modules/**",
      "instrumented/**/!(*.js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://psofleksibel@dev.azure.com/psofleksibel/fp-pso-fleksibel/_git/fp-pso-fleksibel"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.4",
    "@eslint/js": "^9.27.0",
    "cypress": "^14.4.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "http-server": "^14.1.1",
    "nyc": "^17.1.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0"
  }
}
